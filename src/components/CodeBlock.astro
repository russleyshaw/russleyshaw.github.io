---
import { splitLines } from "../lib/string";

interface CodeLine {
  text: string;
  isAdded: boolean;
  isRemoved: boolean;
  isModified: boolean;

  isHidden: boolean;
}

export interface Props {
  code: string;
}

const lines: CodeLine[] = [];
---

<pre>
    {lines.map((line) => <span>{line.text}</span>)}
</pre>
